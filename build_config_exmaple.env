# Resistine AI - Build Configuration Template
# Copy this file to build_config.env and fill in your credentials

# Signing Configuration
# Get your certificate ID with: security find-identity -v -p codesigning
SIGNING_IDENTITY=""

# Notarization Configuration
# Your Apple ID email address
NOTARY_APPLE_ID=""

# Your Team ID (found in Apple Developer portal)
NOTARY_TEAM_ID=""

# Your app-specific password (generate at appleid.apple.com)
NOTARY_PASSWORD=""

# App Configuration
APP_NAME="Resistine AI"
APP_VERSION="1.0.0"
BUNDLE_ID="com.resistine.ai"

# Build Options
SKIP_SIGNING=false
SKIP_NOTARIZATION=false 